{
  "name": "next-super-layout",
  "version": "0.1.3",
  "description": "Convenient layout routes & React hooks for Next.js",
  "author": "Ian Smith <hello@iks.codes>",
  "repository": "smithki/next-super-layout",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.js"
  },
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "yarn clean && ./scripts/build.ts --watch",
    "build": "yarn clean && ./scripts/build.ts",
    "clean": "rimraf dist",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "test": "echo \"No unit tests... yet :(\" && exit 0",
    "test:tree-shaking": "yarn build && agadoo"
  },
  "devDependencies": {
    "@ikscodes/eslint-config": "latest",
    "@ikscodes/prettier-config": "latest",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "agadoo": "^2.0.0",
    "auto": "^10.32.6",
    "esbuild": "^0.14.23",
    "eslint": "^7.14.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fast-glob": "^3.2.11",
    "next": "^12.1.0",
    "prettier": "^2.2.0",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-morph": "^13.0.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "next": "^12.1.0",
    "react": "^17.0.2"
  },
  "packageManager": "yarn@3.2.0",
  "prettier": "@ikscodes/prettier-config",
  "auto": {
    "plugins": [
      "npm"
    ]
  }
}
